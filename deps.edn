{:paths ["config" "src" "resources"]
 :deps  {com.rpl/specter                {:mvn/version "1.1.3"}
         expound/expound                        {:mvn/version "0.8.9"}
         com.bhauman/spell-spec         {:mvn/version "0.1.2"}
         org.clojure/test.check         {:mvn/version "1.1.0"
                                         :scope       "test"}}
 :aliases
        {:ancient {:main-opts  ["-m" "deps-ancient.deps-ancient"]
                   :extra-deps {deps-ancient/deps-ancient {:mvn/version "0.0.5"}}}
         :cljfmt {:main-opts ["-m" "cljfmt.main" "fix"]     ;; -m cljfmt.main [check|fix]
                  :extra-deps {cljfmt/cljfmt {:mvn/version "0.7.0"}}}
         :test    {:extra-paths ["test"]
                   :extra-deps  {org.clojure/test.check {:mvn/version "1.1.0"}}}
         :runner
                  {:extra-deps {com.cognitect/test-runner
                                {:git/url "https://github.com/cognitect-labs/test-runner"
                                 :sha     "f7ef16dc3b8332b0d77bc0274578ad5270fbfedd"}}
                   :main-opts  ["-m" "cognitect.test-runner"
                                "-d" "test"]}}}
